{
    "name": "b2pweb/bdf-prime-analyzer",
    "type": "library",
    "description": "Analyze queries generated by Prime ORM and dump unsafe or not optimized queries",
    "keywords": ["prime", "orm", "analyzer"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Quatrevieux",
            "email": "vquatrevieux@b2pweb.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bdf\\Prime\\Analyzer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnalyzerTest\\": "tests/_files"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "~8.0.0 || ~8.1.0 || ~8.2.0",
        "ext-json": "*",
        "b2pweb/bdf-prime": "^2.0.5",
        "b2pweb/bdf-collections": "~1.1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.7"
    },
    "scripts": {
        "tests": "phpunit",
        "tests-with-coverage": "phpunit --coverage-clover coverage.xml",
        "psalm": "psalm --shepherd --php-version=8.0"
    }
}
